<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.fg.footballgames.Admin.Controllers.AdminMainPageController"
            prefHeight="720.0" prefWidth="1280.0">
    <children>
        <BorderPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0" AnchorPane.topAnchor="0.0">
            <top>
                <AnchorPane prefHeight="27.0" prefWidth="600.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="accountSettingsButton" layoutX="56.0" layoutY="1.0" mnemonicParsing="false" onAction="#accountSettingsButtonPressed" text="Account Settings" />
                        <Button fx:id="aboutButton" layoutY="1.0" mnemonicParsing="false" onAction="#aboutButtonPressed" text="About" />
                        <Button fx:id="mainPageButton" layoutX="300.0" layoutY="1.0" mnemonicParsing="false" onAction="#mainPageButtonPressed" text="Main Page" />
                        <Button fx:id="logoutButton" onAction="#logoutButtonPressed" layoutX="168.0" layoutY="1.0" mnemonicParsing="false" text="Logout" />
                    </children>
                </AnchorPane>
            </top>
        </BorderPane>
    </children>
</AnchorPane>

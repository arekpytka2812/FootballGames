   <children>
      <TableView fx:id="playersTableView" layoutX="20.0" layoutY="180.0" prefHeight="480.0" prefWidth="1240.0" />
      <RadioButton fx:id="allClubsRadio" layoutX="938.0" layoutY="68.0" mnemonicParsing="false" onAction="#allClubsRadioClicked" text="all clubs" />
      <RadioButton fx:id="chosenClubRadio" layoutX="938.0" layoutY="101.0" mnemonicParsing="false" onAction="#chosenClubRadioClicked" text="chosen club:" />
      <Label layoutX="938.0" layoutY="40.0" text="Select players from:" />
      <ChoiceBox fx:id="chosenClubChoiceBox" disable="true" layoutX="1042.0" onAction="#chosenClubChoiceBoxSelected" layoutY="97.0" prefWidth="150.0" />
   </children>
</AnchorPane>
